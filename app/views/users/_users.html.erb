<% if users.any? %>
  <ul class="list-group list-group-flush" id="user">
    <% users.each do |user| %>
      <li class="list-group-item d-flex justify-content-between align-items-center">
        <h4 class="mr-5"><%= link_to user.name, user_path(user) %></h4>
        <span class="badge"><%= render 'relationships/follow_button', user: user %></span>
      </li>
    <% end %>
  </ul>
  <%= paginate users %>
<% end %>


